      "*": [
        "dist/index.d.ts"
      ],
      "dist/common": [
        "dist/common.d.ts"
      ],
      "dist/server": [
        "dist/server.d.ts"
      ],
      "dist/builder": [
        "dist/builder.d.ts"
      ],
      "dist/telemetry": [
        "dist/telemetry.d.ts"
      ]
    }
  },
  "files": [
    "dist/**/*",
    "templates/**/*",
    "public/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "check": "../../../scripts/node_modules/.bin/tsc --noEmit",
