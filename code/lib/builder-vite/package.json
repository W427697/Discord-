    "glob": "^8.1.0",
    "glob-promise": "^6.0.2",
    "magic-string": "^0.27.0",
    "remark-external-links": "^8.0.0",
    "remark-slug": "^6.0.0",
    "rollup": "^2.25.0 || ^3.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.0.0",
    "rollup": "^3.20.1",
    "slash": "^5.0.0",
    "typescript": "~4.9.3",
    "vite": "^4.0.4"
  },
  "peerDependencies": {
    "@preact/preset-vite": "*",
    "typescript": ">= 4.3.x",
    "vite": "^3.0.0 || ^4.0.0",
    "vite-plugin-glimmerx": "*"
  },
  "peerDependenciesMeta": {
    "@preact/preset-vite": {
      "optional": true
