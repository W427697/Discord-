    "@junk-temporary-prototypes/csf-plugin": "7.2.0-alpha.3",
    "@junk-temporary-prototypes/node-logger": "7.2.0-alpha.3",
    "@junk-temporary-prototypes/preview": "7.2.0-alpha.3",
    "@junk-temporary-prototypes/preview-api": "7.2.0-alpha.3",
    "@junk-temporary-prototypes/types": "7.2.0-alpha.3",
    "@storybook/mdx2-csf": "^1.0.0",
