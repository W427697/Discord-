  },
  "dependencies": {
    "@junk-temporary-prototypes/channel-postmessage": "7.2.0-alpha.3",
    "@junk-temporary-prototypes/channel-websocket": "7.2.0-alpha.3",
    "@junk-temporary-prototypes/client-logger": "7.2.0-alpha.3",
